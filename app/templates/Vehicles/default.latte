{block content}
<h1>Vozidla</h1>

{foreach $vehicleslist as $v}
{first 4}<div class="row">{/first}
<div class="col-md-2">
	<a n:href="Vehicle:, id=>$v->id">
    	{if $v['profilpic'] != ""}
    		<img src="{$basePath}/data/profil/{$v['profilpic']}" alt="profil" width="140" height="140">
    	{else}
    		<img data-src="{$basePath}/js/holder.js/140x140" alt="profil" width="140" height="140">
    	{/if}
    	{$v->name}
    </a>
</div>
{last 4}</div>{/last}
{/foreach}

<div class="row">
  <div class="col-md-3">
      <ul class="nav nav-pills nav-stacked">
	  <li {ifCurrent passwordChange}class="active"{/ifCurrent}><a n:href="add">PÅ™idat vozidlo</a></li>
      </ul>
  </div>
  <div class="col-md-9">
      {block con}{/block}
  </div>
</div>