
{block cont}
{foreach $members as $m}
{first 4}<div class="row">{/first}
<div class="col-md-2">
    <a n:href="User:, id=>$m->user->id">
    {if $m->user->profilpic != ""}
    <img src="{$basePath}/data/profil/{$m->user->profilpic}" alt="profil" width="140" height="140">
    {else}
    <img data-src="{$basePath}/js/holder.js/140x140" alt="profil" width="140" height="140">
    {/if}
    {$m->user->email}
    </a>
</div>
{last 4}</div>{/last}
{/foreach}